<AnchorPane id="AnchorPane" prefHeight="803.0" prefWidth="1300.0" style="-fx-background-image: url(&quot;/Assets/LoginPage.jpeg&quot;);" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginPageController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="551.0" layoutY="391.0" onAction="#LogIn" prefHeight="53.0" prefWidth="199.0" promptText="Enter your password" styleClass="passfield" stylesheets="@../CSS/design.css" />
      <Button fx:id="LoginButton" layoutX="596.0" layoutY="561.0" mnemonicParsing="false" onAction="#LogIn" prefHeight="53.0" prefWidth="109.0" styleClass="loginButton" stylesheets="@../CSS/design.css" text="Log In">
